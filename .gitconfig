[alias]
  s = status --branch
  d = diff
  ds = diff --staged
  c = commit
  p = push
  a = add
  aa = add -A .
  lo = log --graph --pretty=format:'%C(red)%h %C(bold blue)<%an> %C(green)(%cr)%C(auto)%d %C(reset)%s'
  ch = checkout
  l = log
  f = fetch
  fa = fetch --all
  fap = fetch --all --prune
  ap = add --patch
  branches = for-each-ref --sort=-committerdate --count 50 --format=\"%(align:width=20)%(color:blue)%(committerdate:relative)%(end)%(align:width=20)%(color:red)%(authorname)%(end)%(color:white)%(color:bold)%(refname:short)\" refs/remotes
  local-branches = for-each-ref --sort=-committerdate --count 50 --format=\"%(align:width=20)%(color:blue)%(committerdate:relative)%(end)%(align:width=20)%(color:red)%(authorname)%(end)%(color:white)%(color:bold)%(refname:short)\" refs/heads
  cane = commit --amend --no-edit
  ca = commit --amend
  pt = push --tags
  ptf = push --tags -f
  tas = tag -a -s
  pf = push -f
[user]
  email = jakubkalab@gmail.com
  name = Jakub Kal√°b
  ; signingKey = CF312A94FC2548AD
; [diff]
;   external = difft
[merge]
  ; conflictstyle = diff3
  conflictstyle = zdiff3
[pull]
  ff = only
[commit]
  ; gpgsign = true
[checkout]
  defaultRemote = origin
[core]
  ; pager = less -MRFX
  pager = delta
  excludesFile = ~/.gitignore
[interactive]
  diffFilter = delta --color-only
[delta]
  navigate = true  # use n and N to move between diff sections
  dark = true      # or light = true, or omit for auto-detection
  line-numbers = true
[init]
  defaultBranch = main
[protocol "file"]
  allow = always
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[rerere]
  enabled = true
[rebase]
  instructionFormat = (%an) %s
[trailer "changeid"]
  key = Change-Id
[color]
  ui = auto
[coderabbit]
  machineId = cli/6d6dc78d3989407093536cffb766e0a7
[credential]
	helper = libsecret
